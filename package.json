{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "bun-sveltekit-todo",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "bunx --bun vite build",
        "check": "bunx --bun svelte-check",
        "db:generate": "bunx drizzle-kit generate",
        "db:migrate": "bunx --bun drizzle-kit migrate",
        "dev": "bunx --bun vite dev",
        "format": "bunx --bun prettier --write .",
        "lint": "bunx --bun eslint src",
        "start": "bun run build/index.js"
    },
    "dependencies": {
        "@hono/swagger-ui": "^0.4.1",
        "@hono/zod-openapi": "^0.16.3",
        "argon2": "^0.41.1",
        "drizzle-orm": "^0.33.0",
        "hono": "^4.6.3",
        "http-status-codes": "^2.3.0",
        "postgres": "^3.4.4",
        "redis": "^4.7.0",
        "reflect-metadata": "^0.2.2",
        "tsyringe-neo": "^5.0.3",
        "winston": "^3.15.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.12.0",
        "@sveltejs/kit": "^2.6.1",
        "@sveltejs/vite-plugin-svelte": "^3.1.2",
        "@tsconfig/bun": "^1.0.7",
        "@tsconfig/strictest": "^2.0.5",
        "@types/bun": "^1.1.10",
        "@types/eslint__js": "^8.42.3",
        "@types/http-status-codes": "^1.2.0",
        "@types/node": "^22.7.4",
        "autoprefixer": "^10.4.20",
        "daisyui": "^4.12.12",
        "drizzle-kit": "^0.24.2",
        "eslint": "^9.12.0",
        "eslint-plugin-svelte": "^2.44.1",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.7",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "svelte": "^4.2.19",
        "svelte-adapter-bun": "^0.5.2",
        "svelte-check": "^4.0.4",
        "svelte-eslint-parser": "^0.41.1",
        "svelte-icons-pack": "^3.1.3",
        "tailwindcss": "^3.4.13",
        "typescript": "^5.6.2",
        "typescript-eslint": "^8.8.0",
        "vite": "^5.4.8"
    }
}
